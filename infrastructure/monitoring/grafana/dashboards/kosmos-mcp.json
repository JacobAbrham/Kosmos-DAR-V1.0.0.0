        {
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "drawStyle": "bars",
                        "fillOpacity": 80,
                        "stacking": {
                            "mode": "normal"
                        }
                    },
                    "unit": "short"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 6
            },
            "id": 11,
            "targets": [
                {
                    "expr": "sum(rate(kosmos_mcp_tool_invocations_total[5m])) by (server, tool)",
                    "legendFormat": "{{server}}/{{tool}}"
                }
            ],
            "title": "Tool Invocations by Server/Tool",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "unit": "s"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 6
            },
            "id": 12,
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, sum(rate(kosmos_mcp_tool_duration_seconds_bucket[5m])) by (le, server))",
                    "legendFormat": "{{server}} p95"
                }
            ],
            "title": "Tool Execution Latency (P95)",
            "type": "timeseries"
        },
        {
            "collapsed": false,
            "gridPos": {
                "h": 1,
                "w": 24,
                "x": 0,
                "y": 14
