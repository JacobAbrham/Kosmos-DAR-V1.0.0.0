        {
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "fillOpacity": 80
                    }
                }
            },
            "gridPos": {
                "h": 8,
                "w": 8,
                "x": 0,
                "y": 15
            },
            "id": 21,
            "options": {
                "legend": {
                    "displayMode": "table",
                    "placement": "right"
                },
                "pieType": "pie",
                "reduceOptions": {
                    "calcs": [
                        "sum"
                    ],
                    "values": false
                }
            },
            "targets": [
                {
                    "expr": "sum(kosmos_mcp_tool_invocations_total) by (server)",
                    "legendFormat": "{{server}}"
                }
            ],
            "title": "Invocations by Server",
            "type": "piechart"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 0.01
                            },
                            {
                                "color": "red",
                                "value": 0.05
                            }
                        ]
                    },
                    "unit": "percentunit"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 8,
                "x": 8,
                "y": 15
            },
            "id": 22,
            "targets": [
                {
                    "expr": "sum(rate(kosmos_mcp_tool_errors_total[5m])) by (server) / sum(rate(kosmos_mcp_tool_invocations_total[5m])) by (server)",
                    "legendFormat": "{{server}}"
                }
            ],
            "title": "Error Rate by Server",
            "type": "timeseries"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "decbytes"
                }
            },
            "gridPos": {
                "h": 8,
                "w": 8,
                "x": 16,
                "y": 15
            },
            "id": 23,
            "targets": [
                {
                    "expr": "sum(kosmos_mcp_server_memory_bytes) by (server)",
                    "legendFormat": "{{server}}"
                }
            ],
            "title": "Memory Usage by Server",
            "type": "timeseries"
        }
